{
    "name": "marekskopal/typo3-core",
    "type": "typo3-cms-extension",
    "description": "Core plugin for Marek Skopal's TYPO3 CMS webs",
    "keywords": [
        "TYPO3"
    ],
    "homepage": "https://marekskopal.cz",
    "authors": [
        {
            "name": "Marek Skopal",
            "role": "Developer",
            "homepage": "https://marekskopal.cz"
        }
    ],
    "license": ["GPL-2.0-or-later"],
    "require": {
        "georgringer/news": "^12.0",
        "georgringer/numbered-pagination": "^2.1",
        "netresearch/rte-ckeditor-image": "dev-typo-v13",
        "tpwd/ke_search": "^6.0",
        "typo3/cms-core": "^13.4",
        "typo3/cms-fluid-styled-content": "^13.4",
        "typo3/cms-rte-ckeditor": "^13.4"
    },
    "autoload": {
        "psr-4": {
            "Skopal\\MsCore\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ms_core"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/marekskopal/t3x-rte_ckeditor_image"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^1.12"
    }
}
