{
    "name": "marekskopal/typo3-core",
    "version": "12.4.0",
    "type": "typo3-cms-extension",
    "description": "Core plugin for marekskopal TYPO3 CMS webs",
    "keywords": [
        "TYPO3"
    ],
    "homepage": "https://marekskopal.cz",
    "authors": [
        {
            "name": "Marek Skopal",
            "role": "Developer",
            "homepage": "https://marekskopal.cz"
        }
    ],
    "license": ["GPL-2.0-or-later"],
    "require": {
        "typo3/cms-core": "^12.4.20",
        "typo3/cms-fluid-styled-content": "^12.4",
        "typo3/cms-rte-ckeditor": "^12.4",
        "netresearch/rte-ckeditor-image": "^12.0",
        "georgringer/news": "^11.4",
        "dirkpersky/typo3-rte-ckeditor-fontawesome": "^12.2",
        "tpwd/ke_search": "^5.5",
        "georgringer/numbered-pagination": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Skopal\\MsCore\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ms_core"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    }
}
